generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Page {
  id      Int    @id @default(autoincrement())
  name    String @db.VarChar(250)
  title   String @db.VarChar(250)
  content String
}

model Admin {
  id            Int       @id @default(autoincrement())
  username      String    @unique
  password      String
  email         String    @unique
}
